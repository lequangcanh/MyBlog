<div class="container">
  <div class="row">
    <% if logged_in? %>
      <div class="col-md-4">
        <%= render 'shared/user_info' %>
      </div>

      <div class="col-md-8">
        <%= render 'form_entry' %>
        <%= render 'shared/list_entries' %>
      </div>
    <% else %>
      <%= render 'guess_index' %>
    <% end %>
  </div>
</div>
